@using Umbraco.Cms.Core.Models.Blocks
@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<AboutUs>
@{
    Layout = "MainLayout.cshtml";

    var ceoblocks = Model.Value<IEnumerable<BlockListItem>>("ceo");
    var review = Model.Value<IEnumerable<BlockListItem>>("review");
    var employees = Model.Value<IEnumerable<BlockListItem>>("employees");

}
@* copy this snippet for insertion of partial on other pages *@
<partial name="Partials/_PageBanner" model="(IPageBanner)Model" /> 

@if (ceoblocks?.Any() == true)
{
    foreach (var block in ceoblocks)
    {
        @await Html.PartialAsync("_Ceo", block)
    }
}


@if (review?.Any() == true)
{
    foreach (var block in review)
    {
        @await Html.PartialAsync("_Review", block)
    }
}

@if (employees?.Any() == true)
{
    foreach (var block in employees)
    {
        @await Html.PartialAsync("_Employees", block)
    }
}


<partial name="Partials/_NextProject"/>

