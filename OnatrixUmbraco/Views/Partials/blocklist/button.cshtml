@using Umbraco.Cms.Core.Models.Blocks
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels
@using Umbraco.Cms.Core.Models.PublishedContent
@model BlockListItem

@{
	var c = Model.Content;
	var btnText = c.Value<string>("buttonText");
	var btnLink = c.Value<string>("buttonLink");

}

<a href="@btnLink" class="inline-block my-1 bg-[#4F5955] text-[#F2EDDC] text-sm font-bold px-3 py-2 rounded-sm">@btnText</a>